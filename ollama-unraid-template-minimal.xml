<?xml version="1.0"?>
<Container version="2">
  <Name>ollama</Name>
  <Repository>ollama/ollama</Repository>
  <Registry>https://hub.docker.com/r/ollama/ollama/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/ollama/ollama/</Support>
  <Project>https://github.com/ollama/ollama</Project>
  <Overview>The easiest way to get up and running with large language models locally.</Overview>
  <Category>AI:</Category>
  <WebUI>http://[IP]:[PORT:11434]/</WebUI>
  <TemplateURL/>
  <Icon>https://ollama.ai/public/ollama.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Config" Target="/root/.ollama" Default="/mnt/user/appdata/ollama" Mode="rw" Description="Container Path: /root/.ollama" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/ollama</Config>
  <Config Name="Web Interface" Target="11434" Default="11434" Mode="tcp" Description="Container Port: 11434" Type="Port" Display="always" Required="true" Mask="false">11434</Config>
</Container>
